//printing numbers 1 to 10 using for loop
#include <stdio.h>
  
int main()
{

  // for (initialize expression; test expression; update expression) 
  for (int i = 1; i <= 10; i++)
  {
  // it's the body of the loop. Loop will run until i is greater than or equal to 10
    printf( "\n%d", i);
  }
  return 0;
}

